{
  "name": "my-weather-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44449 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44449 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build",
    "build:ssr": "ng run my-weather-frontend:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "is-ci || cd ../../.. && husky install src/MyWeather.Frontend/ClientApp/.husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0-rc.0",
    "@angular/common": "^16.0.0-rc.0",
    "@angular/compiler": "^16.0.0-rc.0",
    "@angular/core": "^16.0.0-rc.0",
    "@angular/forms": "^16.0.0-rc.0",
    "@angular/platform-browser": "^16.0.0-rc.0",
    "@angular/platform-browser-dynamic": "^16.0.0-rc.0",
    "@angular/platform-server": "^16.0.0-rc.1",
    "@angular/router": "^16.0.0-rc.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0-rc.0",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~16.0.0-rc.0",
    "@angular/compiler-cli": "^16.0.0-rc.0",
    "@ngrx/eslint-plugin": "^15.4.0",
    "@ngrx/schematics": "^15.4.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "run-script-os": "^1.1.6",
    "stylelint": "^15.5.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.1",
    "typescript": "~5.0.2"
  }
}
